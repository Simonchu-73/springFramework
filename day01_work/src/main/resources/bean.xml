<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="teacher" class="com.xzit.model.Teacher">
        <property name="name" value="李四"></property>
        <property name="addr" value="上海市浦东新区"></property>
    </bean>
    <bean id="StudentDaoMysqlImpl" class="com.xzit.dao.impl.StudentDaoMysqlImpl"></bean>
    <bean id="StudentDaoOracleImpl" class="com.xzit.dao.impl.StudentDaoOracleImpl"></bean>
    <bean id="student" class="com.xzit.model.Student">
        <property name="name"  value="张三"></property>
        <property name="age" value="19"></property>
        <property name="teacher" ref="teacher"></property>
    </bean>
</beans>